{
  "schema_version": 1,
  "mixins": [
    "reaping-common.mixins.json",
    "reaping-fabriclike.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "reaping",
    "version": "${version}",
    "name": "Reaping",
    "description": "Harvest your animals in the most cursed way possible.",
    "authors": ["Jamalam"],
    "contact": {
      "homepage": "https://www.curseforge.com/minecraft/mc-mods/reaping",
      "sources": "https://github.com/JamCoreModding/Reaping",
      "issues": "https://github.com/JamCoreModding/Reaping/issues"
    },
    "license": "MIT",
    "icon": "assets/reaping/icon.png",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "environment": "*",
    "entrypoints": {
      "init": [
        "io.github.jamalam360.reaping.quilt.ReapingQuilt"
      ],
      "modmenu": [
        "io.github.jamalam360.reaping.fabriclike.ModMenuIntegration"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.18.2"
      },
      {
        "id": "architectury",
        "version": ">=4.2.50"
      }
    ]
  }
}